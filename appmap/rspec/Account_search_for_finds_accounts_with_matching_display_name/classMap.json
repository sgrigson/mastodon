        "name": "SELECT accounts.*, ( (( greatest(0, coalesce(s.followers_count, 0)) / ( greatest(0, coalesce(s.following_count, 0)) + 1.0 ) ) + log( greatest(0, coalesce(s.followers_count, 0)) + 2 ) + ( case when s.last_status_at is null then 0 else exp( -1.0 * ( ( greatest(0, abs(extract(DAY FROM age(s.last_status_at))) - 30.0)^2) / (2.0 * ((-1.0 * 30^2) / (2.0 * ln(0.3))) ) ) ) end )) / 3.0 ) * ts_rank_cd(( setweight(to_tsvector('simple', accounts.display_name), 'A') || setweight(to_tsvector('simple', accounts.username), 'B') || setweight(to_tsvector('simple', coalesce(accounts.domain, '')), 'C') ), to_tsquery('simple', ''' display '':*'), 32) AS rank FROM accounts LEFT JOIN users ON accounts.id = users.account_id LEFT JOIN account_stats AS s ON accounts.id = s.account_id WHERE to_tsquery('simple', ''' display '':*') @@ ( setweight(to_tsvector('simple', accounts.display_name), 'A') || setweight(to_tsvector('simple', accounts.username), 'B') || setweight(to_tsvector('simple', coalesce(accounts.domain, '')), 'C') ) AND accounts.suspended_at IS NULL AND accounts.moved_to_account_id IS NULL AND (accounts.domain IS NOT NULL OR (users.approved = TRUE AND users.confirmed_at IS NOT NULL)) ORDER BY rank DESC LIMIT 10 OFFSET 0"
