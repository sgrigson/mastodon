        "type": "query",
        "name": "SELECT \"statuses\".\"id\" FROM \"statuses\" WHERE \"statuses\".\"account_id\" = $1 AND \"statuses\".\"id\" <= $2 AND \"statuses\".\"visibility\" != $3 AND (NOT EXISTS (SELECT 1 FROM status_pins pin WHERE pin.account_id = statuses.account_id AND pin.status_id = statuses.id)) AND \"statuses\".\"poll_id\" IS NULL AND (NOT EXISTS (SELECT 1 FROM media_attachments media WHERE media.status_id = statuses.id)) AND (NOT EXISTS (SELECT 1 FROM favourites fav WHERE fav.account_id = statuses.account_id AND fav.status_id = statuses.id)) AND \"statuses\".\"deleted_at\" IS NULL AND (NOT EXISTS (SELECT 1 FROM bookmarks bookmark WHERE bookmark.account_id = statuses.account_id AND bookmark.status_id = statuses.id)) ORDER BY \"statuses\".\"id\" ASC LIMIT $4"
