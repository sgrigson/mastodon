    "type": "database",
    "name": "Database",
    "children": [
      {
        "type": "query",
        "name": "SELECT \"polls\".* FROM \"polls\" WHERE \"polls\".\"id\" = $1 LIMIT $2"
      },
      {
        "type": "query",
        "name": "SELECT \"mentions\".* FROM \"mentions\" WHERE \"mentions\".\"status_id\" = $1 AND \"mentions\".\"silent\" = $2"
      },
      {
        "type": "query",
        "name": "SELECT \"media_attachments\".* FROM \"media_attachments\" WHERE \"media_attachments\".\"status_id\" = $1 ORDER BY \"media_attachments\".\"id\" ASC"
      },
      {
        "type": "query",
        "name": "SELECT \"tags\".* FROM \"tags\" INNER JOIN \"statuses_tags\" ON \"tags\".\"id\" = \"statuses_tags\".\"tag_id\" WHERE \"statuses_tags\".\"status_id\" = $1"
      },
      {
        "type": "query",
        "name": "SELECT \"statuses\".\"id\", \"statuses\".\"uri\" FROM \"statuses\" WHERE \"statuses\".\"deleted_at\" IS NULL AND \"statuses\".\"account_id\" = $1 AND \"statuses\".\"in_reply_to_id\" = $2 AND \"statuses\".\"visibility\" IN ($3, $4) ORDER BY \"statuses\".\"id\" ASC LIMIT $5"
      }
    ]
